{
  "projects": {
    "default": "valor-software-site"
  },
  "targets": {
    "valor-software-site": {
      "hosting": {
        "development": [
          "valor-software-site"
        ],
        "production": [
          "valor-software-production"
        ],
        "headers": [
          {
            "source": "**",
            "headers": [             
              {
                "key": "X-Frame-Options",
                "value": "deny"
              }       
            ]
          }
        ]
      }
    }
  }
}